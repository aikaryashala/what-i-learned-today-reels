{
  "name": "relano",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preview": "BROWSER=none remotion preview remotion/index.ts",
    "render": "remotion render remotion/index.ts basecomp videos/my-learnings.mp4",
    "generate-yaml": "npx ts-node utils/create-release-notes-yaml.ts",
    "build": "tsc",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "format-check": "echo \"Adding later\"",
    "lint": "echo \"Adding later\" ",
    "package": "ncc build --source-map --license licenses.txt main.ts",
    "test": "jest",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/just-moh-it/relano.git"
  },
  "keywords": [
    "release notes video",
    "whats new video",
    "automatic community management"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@bdsqqq/try": "^2.3.1",
    "@remotion/google-fonts": "3.3.94",
    "@remotion/lambda": "3.3.94",
    "@remotion/media-utils": "3.3.94",
    "@remotion/player": "3.3.94",
    "@remotion/renderer": "3.3.94",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "clsx": "^1.2.1",
    "commander": "^10.0.1",
    "openai": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remotion": "3.3.94",
    "tailwind-merge": "^1.14.0",
    "typescript": "5.0.4",
    "yaml": "^2.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@remotion/bundler": "3.3.94",
    "@remotion/cli": "3.3.94",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.36.1",
    "autoprefixer": "10.4.14",
    "css-loader": "^6.11.0",
    "eslint": "8",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.1",
    "postcss": "8.4.23",
    "postcss-loader": "^7.3.4",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.4",
    "tailwindcss": "3.3.2"
  }
